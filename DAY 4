mysql> SELECT *,
    ->     CASE
    ->         WHEN issue_type LIKE '%software%' THEN 'Software Team'
    ->         WHEN issue_type LIKE '%hardware%' THEN 'Hardware Team'
    ->         WHEN issue_type LIKE '%network%' THEN 'Network Team'
    ->         ELSE 'General Support'
    ->     END AS handling_group
    -> FROM tickets;
