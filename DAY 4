mysql> SELECT *,
    ->     CASE
    ->         WHEN DATE(created_date) = CURDATE() THEN 'New'
    ->         WHEN created_date < CURDATE() - INTERVAL 7 DAY THEN 'Old'
    ->         ELSE 'Recent'
    ->     END AS age_category
    -> FROM tickets;
